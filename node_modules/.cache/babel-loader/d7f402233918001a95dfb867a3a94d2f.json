t>\nexport default {\n  data() {\n    return {\n      isEditing: false,\n      newTodoDescription: \"\"\n    };\n  },\n  props: {\n    description: String,\n    completed: Boolean\n  },\n  methods: {\n    startEditing() {\n      if (this.isEditing) {\n        this.finishEditing();\n      } else {\n        this.newTodoDescription = this.description;\n        this.isEditing = true;\n        this.$nextTick(() => this.$refs.newTodo.focus());\n      }\n    },\n    finishEditing() {\n      this.isEditing = false;\n      this.$emit(\"on-edit\", this.newTodoDescription);\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.completed {\n  text-decoration: line-through;\n}\n</style>"],"sourceRoot":""}]}